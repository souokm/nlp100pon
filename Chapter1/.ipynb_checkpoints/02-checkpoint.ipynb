   "source": [
    "#「パトカー」＋「タクシー」の文字を先頭から交互に連結して文字列「パタトクカシーー」を得よ．"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "str1 = \"パトカー\"\n",
    "str2 = \"タクシー\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "new_str = \"\"\n",
    "char_list = []\n",
    "for i in range(len(str1)):\n",
    "    char_list.append(str1[i])\n",
    "    char_list.append(str2[i])\n",
    "new_str = \"\".join(char_list)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "パタトクカシーー\n"
     ]
    }
   ],
   "source": [
    "print(new_str)"
   ]
  },
  {
